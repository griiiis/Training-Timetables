@model IEnumerable<App.BLL.DTO.ContestType>

@{
    ViewData["Title"] = App.Resources.Views.Shared._Layout.ContestTypes;
}

<h1 class="middle">@App.Resources.Views.Shared._Layout.ContestTypes</h1>


<p>
    <a class="display-6 text-dark" asp-action="Create">@App.Resources.Views.Shared._Layout.Create @App.Resources.Views.Shared._Layout.New</a>
</p>
<br/>
<table class="table table-striped table-hover">
    <thead class="thead-dark">
    <tr>
        <th>@App.Resources.Domain.ContestType.ContestTypeName</th>
        <th>@App.Resources.Domain.ContestType.Description</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
    <tr>
        <td>@item.ContestTypeName</td>
        <td>@item.Description</td>
        <td class="text-end">
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@item.Id">@App.Resources.Views.Shared._Layout.Edit</a>
                <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">@App.Resources.Views.Shared._Layout.Delete</a>
        </td>
    </tr>
    }
    </tbody>
</table>
